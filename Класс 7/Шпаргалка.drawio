        <mxCell id="Ogy-xrGR_8rFZ8SFeMyS-34" value="Ввел пользователь" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=consolas;" vertex="1" parent="1">
