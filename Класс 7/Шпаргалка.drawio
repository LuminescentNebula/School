        <mxCell id="Ogy-xrGR_8rFZ8SFeMyS-36" value="Вывела программа" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontFamily=consolas;" vertex="1" parent="1">
